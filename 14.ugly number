import java.util.*;
class PrimeFactor2 {
    public static void main(String[] args) {
        int number = 14; 
        boolean isUgly = isUglyNumber(number);
        System.out.println(number + " is an ugly number: " + isUgly);
    }

    public static boolean isUglyNumber(int number) {
        if (number <= 0) {
            return false;
        }
        while (number % 2 == 0) {
            number /= 2;
        }
        while (number % 3 == 0) {
            number /= 3;
        }
        while (number % 5 == 0) {
            number /= 5;
        }
        return number == 1;
    }
}
